<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome delectatech{% endblock %}</title>


        <!-- Add stylesheets through webpack entrypoints -->
        {% block stylesheets %}
            {{ encore_entry_link_tags('bootstrap') }}
            {{ encore_entry_link_tags('veltrix') }}
            {{ encore_entry_link_tags('app') }}
            <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.13/css/select2.min.css" rel="stylesheet" />
            <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.13/js/select2.min.js"></script>
        {% endblock %}
        {% block stylesheets_extra %}{% endblock %}
    </head>
    <body class="enlarged">
        {% block body %} {% endblock %}

        <!-- Add javascripts through webpack entrypoints -->
        {% block javascripts %}
            {# Mandatory add this html tag #}
            {{ encore_entry_script_tags('bootstrap') }}
            {{ encore_entry_script_tags('veltrix') }}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
        {% block javascripts_extra %}{% endblock %}
    </body>
</html>
